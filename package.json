{
  "name": "mglue",
  "version": "1.0.0-prerelease.1",
  "description": "tiny game library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/dist/mglue.d.ts",
  "scripts": {
    "start": "node server.js",
    "docs": "typedoc --exclude **/SlideShot.ts --theme minimal --out docs client_src",
    "build_client": "webpack --env.build",
    "build_server": "tsc -p ./tsconfig_server.json",
    "watch_client": "webpack-dev-server --inline --content-base public/",
    "watch_server": "nodemon --watch ./src/server --exec ts-node src/server/server.ts"
  },
  "author": "Jonathan Brodsky",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.91",
    "@types/lz-string": "^1.3.32",
    "awesome-typescript-loader": "^3.2.2",
    "nodemon": "^1.11.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1",
    "webpack-livereload-plugin": "^1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "lz-string": "^1.4.4",
    "tone": "^0.11.11"
  },
  "engines": {
    "node": "6.x"
  }
}
